
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="jumbotron">
    <h1>Event Calendar</h1>
    <div id="calendar"></div>
</div>

@section scripts{

    <script type="text/javascript">


        $(document).ready(function () {
            $('#calendar').FullCalendar({
                theme: true,

                //defaultView: 'agendaDay',
                editable: false,
                events: "/Events/"
            });
        });


    </script>
    }